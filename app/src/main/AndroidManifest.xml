<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="devpaul.business.kidmathadmin">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.KidMathAdmin">
        <activity
            android:name=".activities.news.NewsActivity"
            android:exported="false" />
        <activity
            android:name=".activities.userdetail.StudentDetailActivity"
            android:exported="false" />
        <activity
            android:name=".activities.operations.MulStudentActivity"
            android:exported="false" />
        <activity
            android:name=".activities.operations.DivStudentActivity"
            android:exported="false" />
        <activity
            android:name=".activities.operations.ResStudentActivity"
            android:exported="false" />
        <activity
            android:name=".activities.operations.SumStudentActivity"
            android:exported="false" />
        <activity
            android:name="devpaul.business.kidmathadmin.activities.operations.operationsdetail.ViewHistoricalStudentActivity"
            android:exported="false" />
        <activity
            android:name=".RegisterActivity"
            android:exported="false"
            android:windowSoftInputMode="stateHidden" />
        <activity
            android:name=".HomeActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:windowSoftInputMode="stateHidden">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>